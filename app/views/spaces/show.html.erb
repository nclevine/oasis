<div class='artwork-search-panel'>
  <div class='artwork-search-form'>
    <input value='rembrandt' type='text' class='art-search-input' placeholder='Artist, Location, Motif, Object, Keyword...'>
    <select class='artwork-source'>
      <option>Select A Source</option>
      <option selected value='met'>Met Museum</option>
      <option value='bk'>Brooklyn Museum</option>
      <option value='rijks'>Rijksmuseum</option>
      <option value='coop'>Cooper Hewitt</option>
      <option value='vam'>Victoria & Albert</option>
      <option value='tumblr'>Tumblr</option>
    </select>
    <button class='artwork-search-button'>Find Artwork</button>
    <button class='show-salon-wall'>Return To Salon Wall</button>
  </div>

  <div class='results-container'></div>
  <div class='image-inspector'></div>

  <div class='loading'>
    <h2>LOADING</h2>
  </div>
</div>

<div class='artwork-edit-panel'>
</div>

<div class='edit-tools'>
  <i class="fa fa-search return-to-search"></i>
<!--   <button class='return-to-search'>Search For Artwork</button> -->
  <button class='enter-edit-mode'>Edit Mode</button>
  <button class='exit-edit-mode'>Exit Edit Mode</button>
  <button class='save-salon-wall'>Save Wall</button>
  <button class='enter-inspect-mode'>Inspect Artwork</button>
  <button class='exit-inspect-mode'>Exit Inspect Mode</button>
  <button class='zoom-out'>View Full Wall</button>
  <button class='zoom-in'>Zoom In</button>
</div>
